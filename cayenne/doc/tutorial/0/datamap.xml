<?xml version="1.0" encoding="UTF-8"?>
<data-map>
	<db-entity name="artist" schema="">
		<db-attribute name="DATE_OF_BIRTH" type="TIME" length="8"/>
		<db-attribute name="ARTIST_NAME" type="CHAR" isMandatory="true" length="255"/>
		<db-attribute name="ARTIST_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="11"/>
	</db-entity>
	<db-entity name="auto_pk_support" schema="">
		<db-attribute name="TABLE_NAME" type="CHAR" isMandatory="true" length="100"/>
		<db-attribute name="NEXT_ID" type="INTEGER" isMandatory="true" length="11"/>
	</db-entity>
	<db-entity name="exhibit" schema="">
		<db-attribute name="GALLERY_ID" type="INTEGER" isMandatory="true" length="11"/>
		<db-attribute name="CLOSING_DATE" type="TIME" isMandatory="true" length="8"/>
		<db-attribute name="OPENING_DATE" type="TIME" isMandatory="true" length="8"/>
		<db-attribute name="EXHIBIT_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="11"/>
	</db-entity>
	<db-entity name="gallery" schema="">
		<db-attribute name="GALLERY_NAME" type="VARCHAR" isMandatory="true" length="100"/>
		<db-attribute name="GALLERY_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="11"/>
	</db-entity>
	<db-entity name="painting" schema="">
		<db-attribute name="PAINTING_TITLE" type="VARCHAR" isMandatory="true" length="255"/>
		<db-attribute name="GALLERY_ID" type="INTEGER" length="11"/>
		<db-attribute name="PAINTING_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="11"/>
		<db-attribute name="ESTIMATED_PRICE" type="DECIMAL" length="10"/>
		<db-attribute name="ARTIST_ID" type="INTEGER" length="11"/>
	</db-entity>
	<db-entity name="painting_info" schema="">
		<db-attribute name="TEXT_REVIEW" type="null"/>
		<db-attribute name="PAINTING_ID" type="INTEGER" isPrimaryKey="true" isMandatory="true" length="11"/>
		<db-attribute name="IMAGE_BLOB" type="LONGVARBINARY"/>
	</db-entity>
	<obj-entity name="Artist" className="Artist" dbEntityName="artist">
		<obj-attribute name="dateOfBirth" type="java.sql.Time" db-attribute-name="DATE_OF_BIRTH"/>
		<obj-attribute name="artistName" type="java.lang.String" db-attribute-name="ARTIST_NAME"/>
	</obj-entity>
	<obj-entity name="Exhibit" className="Exhibit" dbEntityName="exhibit">
		<obj-attribute name="galleryId" type="java.lang.Integer" db-attribute-name="GALLERY_ID"/>
		<obj-attribute name="openingDate" type="java.sql.Time" db-attribute-name="OPENING_DATE"/>
		<obj-attribute name="closingDate" type="java.sql.Time" db-attribute-name="CLOSING_DATE"/>
	</obj-entity>
	<obj-entity name="Gallery" className="Gallery" dbEntityName="gallery">
		<obj-attribute name="galleryName" type="java.lang.String" db-attribute-name="GALLERY_NAME"/>
	</obj-entity>
	<obj-entity name="Painting" className="Painting" dbEntityName="painting">
		<obj-attribute name="estimatedPrice" type="java.math.BigDecimal" db-attribute-name="ESTIMATED_PRICE"/>
		<obj-attribute name="galleryId" type="java.lang.Integer" db-attribute-name="GALLERY_ID"/>
		<obj-attribute name="paintingTitle" type="java.lang.String" db-attribute-name="PAINTING_TITLE"/>
		<obj-attribute name="artistId" type="java.lang.Integer" db-attribute-name="ARTIST_ID"/>
	</obj-entity>
	<obj-entity name="PaintingInfo" className="PaintingInfo" dbEntityName="painting_info">
		<obj-attribute name="imageBlob" type="byte[]" db-attribute-name="IMAGE_BLOB"/>
		<obj-attribute name="textReview" type="java.lang.Integer" db-attribute-name="TEXT_REVIEW"/>
	</obj-entity>
</data-map>
