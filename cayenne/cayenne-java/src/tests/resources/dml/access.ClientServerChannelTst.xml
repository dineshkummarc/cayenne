<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans default-lazy-init="true">	
	<!-- ======================================= -->
	<!-- Named Queries -->
	<!-- ======================================= -->
	
	<bean id="DELETE_MT_TABLE2" class="org.objectstyle.cayenne.unit.util.UpdatingSQLTemplate">
		<constructor-arg type="java.lang.Class"><value>org.objectstyle.cayenne.testdo.mt.MtTable2</value></constructor-arg>
		<constructor-arg><value>
		delete from MT_TABLE2
		</value></constructor-arg>
	</bean>
	
	<bean id="DELETE_MT_TABLE1" class="org.objectstyle.cayenne.unit.util.UpdatingSQLTemplate">
		<constructor-arg type="java.lang.Class"><value>org.objectstyle.cayenne.testdo.mt.MtTable1</value></constructor-arg>
		<constructor-arg><value>
		delete from MT_TABLE1
		</value></constructor-arg>
	</bean>
	
	<bean id="DELETE_MT_TABLE3" class="org.objectstyle.cayenne.unit.util.UpdatingSQLTemplate">
		<constructor-arg type="java.lang.Class"><value>org.objectstyle.cayenne.testdo.mt.MtTable3</value></constructor-arg>
		<constructor-arg><value>
		delete from MT_TABLE3
		</value></constructor-arg>
	</bean>
	
	<bean id="INSERT_MT_TABLE11" class="org.objectstyle.cayenne.unit.util.UpdatingSQLTemplate">
		<constructor-arg type="java.lang.Class"><value>org.objectstyle.cayenne.testdo.mt.MtTable1</value></constructor-arg>
		<constructor-arg><value>
		insert into MT_TABLE1 (TABLE1_ID, GLOBAL_ATTRIBUTE1, SERVER_ATTRIBUTE1) values (55, 'g1', 's1')
		</value></constructor-arg>
	</bean>
	
	<bean id="INSERT_MT_TABLE12" class="org.objectstyle.cayenne.unit.util.UpdatingSQLTemplate">
		<constructor-arg type="java.lang.Class"><value>org.objectstyle.cayenne.testdo.mt.MtTable1</value></constructor-arg>
		<constructor-arg><value>
		insert into MT_TABLE1 (TABLE1_ID, GLOBAL_ATTRIBUTE1, SERVER_ATTRIBUTE1) values (65, #bind($GLOBAL_ATTRIBUTE1), #bind($SERVER_ATTRIBUTE1))
		</value></constructor-arg>
	</bean>
	
	<bean id="INSERT_MT_TABLE31" class="org.objectstyle.cayenne.unit.util.UpdatingSQLTemplate">
		<constructor-arg type="java.lang.Class"><value>org.objectstyle.cayenne.testdo.mt.MtTable3</value></constructor-arg>
		<constructor-arg><value>
		insert into MT_TABLE3 (TABLE3_ID, BINARY_COLUMN, CHAR_COLUMN, INT_COLUMN) values (1, #bind($bytes), #bind($chars), 4)
		</value></constructor-arg>
	</bean>
	

	
	<!-- ======================================= -->
	<!-- Data Sets -->
	<!-- ======================================= -->	
	<bean id="testOnSelectQueryObjectIDInjection" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<ref bean="DELETE_MT_TABLE2"/>
				<ref bean="DELETE_MT_TABLE1"/>
				<ref bean="INSERT_MT_TABLE11"/>
			</list>
		</constructor-arg>
	</bean>
	
	<bean id="testOnSelectQueryValuePropagation" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<ref bean="DELETE_MT_TABLE3"/>
				<ref bean="INSERT_MT_TABLE31"/>
			</list>
		</constructor-arg>
	</bean>
	
	<bean id="testOnSelectQueryValuePropagationInheritance" class="java.util.ArrayList">
		<constructor-arg>
			<list>
				<ref bean="DELETE_MT_TABLE2"/>
				<ref bean="DELETE_MT_TABLE1"/>
				<ref bean="INSERT_MT_TABLE12"/>
			</list>
		</constructor-arg>
	</bean>
</beans>